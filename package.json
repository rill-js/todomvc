{
  "description": "Isomorphic TodoMVC example with Rill",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "@rill/body": "^1.1.1",
    "@rill/html": "^2.0.8",
    "@rill/logger": "^1.0.1",
    "@rill/progress": "^1.0.1",
    "@rill/redirect": "^0.2.0",
    "@rill/session": "^5.0.2",
    "@rill/set": "^0.1.0",
    "@rill/static": "^0.5.0",
    "as-html": "^2.0.0",
    "babel-polyfill": "^6.9.1",
    "babel-register": "^6.9.0",
    "classnames": "^2.2.5",
    "rill": "^0.6.6",
    "todomvc-app-css": "^2.0.6",
    "todomvc-common": "^1.0.2",
    "uid": "0.0.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1"
  },
  "private": true,
  "scripts": {
    "build": "test -d public || mkdir public; browserify js/app.js -t [babelify] > public/bundle.js",
    "start": "npm run build && node -r babel-register js/app.js"
  }
}
